  },
  "homepage": "https://github.com/dagreatcode/whatsfordinner2.0#readme",
  "dependencies": {
    "axios": "^0.27.2",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "if-env": "^1.0.4",
    "mongoose": "^6.6.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "concurrently": "^7.4.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "mocha": "^10.0.0",
    "newman": "^5.3.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1"
  }
}
